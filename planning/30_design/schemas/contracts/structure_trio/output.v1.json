{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "StructureTrioOutput",
  "type": "object",
  "required": ["status", "structure"],
  "properties": {
    "status": {"type": "string", "enum": ["verified_exact", "verified_equivalent", "needs_revision"]},
    "structure": {
      "type": "object",
      "properties": {
        "structure_key": {"type": "string"},
        "levels": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "level_id": {"type": "string"},
              "allowed_sets": {
                "type": "array",
                "items": {"type": "string"}
              }
            }
          }
        }
      }
    },
    "verification_report_ref": {"type": "string"}
  }
}
